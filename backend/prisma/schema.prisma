generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model car_info {
  id              Int    @id @default(autoincrement())
  cost            Int
  city_mpg        Int
  class_type      String
  combination_mpg Int
  drive           String
  fuel_type       String
  highway_mpg     Int
  make            String
  name            String
  system          String
  model           String
  transmission    String
  image           String
}
